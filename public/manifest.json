{
    "manifest_version": 2,
    "name": "Rete Academy",
    "author": "Sang Dang",
    "version": "1.0.0",
    "description": "Another way to learn.",
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Rete Academy"
    },
    "permissions": [
        "storage",
        "http://localhost:8000",
        "https://api.rete.academy"
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}
